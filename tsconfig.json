{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "es5",
    "declaration": true,
    "module": "commonjs",
    "lib": ["es2020", "dom"],
    "strict": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "jsx": "react",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "noUnusedLocals": true
  },
  "exclude": ["node_modules"]
}
